    <script>
      document.addEventListener('DOMContentLoaded', function () {
        var acc = document.getElementsByClassName('accordion_cv');
        var panels = document.getElementsByClassName('panel_cv');
        var accordionContainer = document.querySelector('.cv_accordion'); // Assuming .cv_accordion is a container for the accordion
        // Open the first accordion by default
        if (acc.length > 1) {
          acc[0].classList.add('active');
          panels[0].style.maxHeight = panels[0].scrollHeight + 'px';
        }
        for (var i = 0; i < acc.length; i++) {
          acc[i].addEventListener('click', function () {
            // Close all other panels
            for (var j = 0; j < acc.length; j++) {
              if (acc[j] !== this) {
                acc[j].classList.remove('active');
                panels[j].style.maxHeight = null;
              }
            }
            // Toggle the clicked panel
            this.classList.toggle('active');
            var panel = this.nextElementSibling;
            // Scroll to the .cv_accordion container when the panel is opened
            if (panel.style.maxHeight) {
              panel.style.maxHeight = null;
            } else {
              panel.style.maxHeight = panel.scrollHeight + 'px';
              // Scroll to the .cv_accordion container with a 30px offset
              if (accordionContainer) {
                // Calculate the position with a 30px top margin
                window.scrollTo({
                  top: accordionContainer.offsetTop - 90, // Subtract 30px from the top offset
                  behavior: 'smooth' // Smooth scrolling
                });
              }
            }
          });
        }
      });
    </script>
