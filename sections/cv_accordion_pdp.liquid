<div class="cv_accordion">
  <style>
    .accordion_cv {
      background-color: #eee;
      color: #444;
      cursor: pointer;
      padding: 18px;
      width: 100%;
      border: none;
      text-align: left;
      outline: none;
      font-size: 15px;
      transition: 0.4s;
    }

    .accordion_cv.active,
    .accordion_cv:hover {
      background-color: #ccc;
    }
    /* .accordion_cv:after {
      content: '\002B';
      color: #777;
      font-weight: bold;
      float: right;
      margin-left: 5px;
    }

    .accordion_cv.active:after {
      content: '\2212';
    } */
    .panel_cv {
      padding: 0;
      background-color: white;
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.2s ease-out;
    }
  </style>
  <div class="cv_accordion_main">
    <div class="cv_accordion_heading">
      {{ section.settings.heading }}
    </div>
