        <div class="Testimonials_Main">
          <p class="Testimonials_Main_Title">Reviews</p>
          <div class="Testimonials_Slider">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'testimonials' -%}
                   <div class="Testimonials_Wrapper">
                     {% if block.settings.testimonial_video != blank %}
                      <video class="hide-mobile" controls muted loop preload="metadata">
                        <source src="{{ block.settings.testimonial_video }}#t=0.1" type="video/mp4">
