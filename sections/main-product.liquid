                  <button class="faq__header">
                    <p class="sh4 medium">Ingredients</p>
                    {% render 'icon-caret' %}
                  </button>
                  <div class="faq__content">
                    <div class="sh4 single_line_text_field">{{ product.metafields.custom.new_ingredients_tab }}</div>
